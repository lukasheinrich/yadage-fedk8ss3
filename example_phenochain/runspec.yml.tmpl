dataarg: private_fs_public_obj:$__BUCKET__
dataopts:
  type: s3
  access_key: $__ACCESSKEY__
  secret_key: $__SECRETKEY__
  host: $__HOST__
  local: /work

backend: kube-extrest-obj
backendopts:
  purepubopts:
    exec: {logging: false}
  resultstore:
    access_key: $__ACCESSKEY__
    secret_key: $__SECRETKEY__
    host: $__HOST__
    bucket: $__BUCKET__
  kubeconfig: fromenv

plugins:
- yadagekube.backends
- yadageobjstore.register_yadage
- yadageobjstore.register_packtivity

updateinterval: 10
ctrlopts:
  disable_prepub: true

initdata:
  nevents: 100

workflow: madgraph_delphes.yml
metadir: here
visualize: true
